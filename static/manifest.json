{
  "name": "Saka",
  "version": "0.2.0",
  "author": "Sufyan Dawoodjee",
  "description": "Saka - elegent tab search, selection, and beyond",
  "manifest_version": 2,
  "background": {
    "scripts": ["background_page.js"],
    "persistent": true
  },
  "commands": {
    "toggleSaka": {
      "suggested_key": {
        "default": "Alt+Space"
      },
      "description": "Toggle Saka"
    },
    "toggleSaka2": {
      "suggested_key": {
        "default": "Ctrl+K"
      },
      "description": "Toggle Saka"
    },
    "toggleSaka3": {
      "suggested_key": {
        "default": "Ctrl+E"
      },
      "description": "Toggle Saka"
    },
    "toggleSaka4": {
      "suggested_key": {
        "default": "Ctrl+Shift+1"
      },
      "description": "Toggle Saka",
      "global": true
    }
  },
  "browser_action": {
    "default_icon": {
      "48": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
    },
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab",
    "history",
    "bookmarks",
    "chrome://favicon/"
  ],
  "web_accessible_resources": [
    "saka.html",
    "material-icons.css",
    "MaterialIcons-Regular.woff2"
  ]
}
