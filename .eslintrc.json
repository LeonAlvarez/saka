{
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }]
  },
  "extends": ["plugin:prettier/recommended"],
  "env": {
    "webextensions": true
  },
  "globals": {
    "document": true,
    "navigator": false,
    "window": true,
    "SAKA_DEBUG": true,
    "SAKA_PLATFORM": true,
    "SAKA_VERSION": true,
    "SAKA_BENCHMARK": true
  }
}
